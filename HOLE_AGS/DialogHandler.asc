        // Short repeat dialog
        player.Say("Scan completed and results transmitted");
      }
    }
    audioSFX = aRadio_close.Play(eAudioPriorityHigh, eOnce);
    return;
  }
  
  
// Miscallaneous dialog
  
  if (DialogID == 200) { // Trash scan result
    int trashrandom = Random(6);
    if (trashrandom == 0) {player.Say("Doesn't look very important");}
    if (trashrandom == 1) {player.Say("Not worth scanning");}
    if (trashrandom == 2) {player.Say("That doesn't seem worthwhile");}
    if (trashrandom == 3) {player.Say("Not valuable, let's move on");}
    if (trashrandom == 4) {player.Say("Not sure what that is, but it's not important");}
    if (trashrandom == 5) {player.Say("Nah, nothing here");}
    if (trashrandom == 6) {player.Say("Nope, got nothing of value");}
    audioSFX = aRadio_close.Play(eAudioPriorityHigh, eOnce);
    return;
  }
  
  if (DialogID == 201) { // Repeated scan result
    player.Say("Already scanned this one, nothing new here");
    audioSFX = aRadio_close.Play(eAudioPriorityHigh, eOnce);
    return;
  }
  
  
  // If we reached here, we called for a non-existent dialog ID
  player.Say("I feel like talking to myself...");
  audioSFX = aRadio_close.Play(eAudioPriorityHigh, eOnce);
  return;
} export TriggerDialog;